16 71 crange

m:
  1000 double + show
  9
  push-b show
  new-line

    # c = cmp((Y - 29) / 6, 0) + 1
    @2 29 -
      6 /
      sign inc

    1000 *

    @0
      sign inc
      7900
    *

    # A = 1000 * c
    +

    push-b +

    # x += x / 118
    dup 118 / +
    pop-b
